//Floyd
#include <iostream>
#include <algorithm>
using namespace std;

void fun(int M[6][6]){
    int a,b;
    
    for(int k=1; k<6; k++){
        for(int i=1; i<6; i++){
            for(int j=1; j<6; j++){
             a = M[i][j];
             b = M[i][k] + M[k][j];
    
            M[i][j] = min(a,b);
            }
        }
    }
    
};

void display(int M[6][6]){
    for(int i=1; i<6; i++){
        for(int j=1; j<6; j++){
            cout<<M[i][j]<<", ";
        }
        cout<<endl;
    }
    
}

int main() {

    
int M[6][6];
M[1][1]=0; M[1][2]=3; M[1][3]=8; M[1][4]=9999; M[1][5]=-4;
M[2][1]=9999; M[2][2]=0; M[2][3]=9999; M[2][4]=1; M[2][5]=7;
M[3][1]=9999; M[3][2]=4; M[3][3]=0; M[3][4]=9999; M[3][5]=9999;
M[4][1]=2; M[4][2]=9999; M[4][3]=-5; M[4][4]=0; M[4][5]=9999;
M[5][1]=9999; M[5][2]=9999; M[5][3]=9999; M[5][4]=6; M[5][5]=0;

    fun(M);
    display(M);

    return 0;
}
